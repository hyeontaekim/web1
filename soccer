	_param1 = '',
	_param2 = '';

var _time = '1609822210',
    _time_d = '20210105',
    _referer = 'https%3A%2F%2Fwww.google.com%2F';
	
var _ad_position = '',
	_ad_idx = '',
	_ad_cnt = '1';

// var _ad_hidden_article = json_encode ($_AD_HIDDEN_ARTICLE_ARR),
//	_ad_hidden_photo = json_encode ($_AD_HIDDEN_PHOTO_ARR);
	
// 엔터 체크
function pressCheck(e) {
	
	//alert("pressCheck");
	//alert(typeof(e));
	//파이어폭스
	if(typeof(e) != "undefined"){
		//alert("파이어폭스");
		if(e.which == 13) {
			return doSearch();
		}else{
			return false;
		}
	//익스플로러
	}else{
		//alert("explolor");
		if (event.KeyCode == 13) {
			return doSearch();
		}else{
			return false;
		}
	}
}
//검색
function doSearchMain() {
	var searchForm = document.pront_top_search; 

	if (searchForm.query.value == "") {
		alert("검색어를 입력하세요.");
		searchForm.query.focus();
		return false;
	}

	// 수능 특집 2014-11-13
	if(searchForm.query.value == "수능 생생 화보") {
		searchForm.action = "//www.news1.kr/photos/gallery_view/?448";
	}

	searchForm.collection.value = "";
	searchForm.startDate.value = "";
	searchForm.endDate.value = "";
	searchForm.cal_range.value = "";
	searchForm.startCount.value = "";
	searchForm.searchField.value = "";
	searchForm.sort.value = "DATE";
	searchForm.listType.value = "";

	//doReset();
	return true;
}

function doSearch() {
	var searchForm = document.pront_top_search; 

	if (searchForm.query.value == "") {
		alert("검색어를 입력하세요.");
		searchForm.query.focus();
		return false;
	}
	
	searchForm.collection.value = "ALL";
	searchForm.startDate.value = "";
	searchForm.endDate.value = "";
	searchForm.cal_range.value = "A";
	searchForm.startCount.value = 0;
	searchForm.searchField.value = "ALL";
	searchForm.sort.value = "DATE";
	searchForm.listType.value = "listType1";

	searchForm.submit();
}
</script>

<script type="text/javascript">
$(document).ready(function(){

	// 브라우저 타입을 body에 추가
	$('body').addClass(getBrowserType());
	
	// 속보
	$.ajax({
		method: "GET",
		crossDomain: true,
		url: "https://rest.news1.kr/w/ajax/extcontent?type=breaking_news",
		dataType : "json",
		cache: false
	}).done(function(data) {
		var json = data,
			$list = $('#newsticker1 .newsflash_list');
		
		if (json.size >= 1 && json.data.length >= 1) {
			
			for (var i in json.data) {
				var val = json.data[i],
					url = "/articles/?" + val.article_id,
					title = val['title'];

				var $item = $("<li class=\"ellipsis\"><a href=\"" + url + "\">" + title + "</a></li>"); 
				$list.cycle('add', $item);
			}
		}
	});

});
</script>

