<script>
$(document).ready(function() {

// 	블로그칼럼
	$.ajax({
		method: "GET",
		url: _rest_url_+"/w/ajax/column/recent",
		dataType : "json",
		cache: false
	}).done(function(res) {
		var $list1 = $('#AsideColumnExpert');
		var $list2 = $('#AsideColumnOuter');
		var $list3 = $('#AsideColumnInner');

		var data = res.expert;
		if (_.size(data) >= 1 && _.size(data) >= 1) {
            _.each(data,function (val) {
				var html ='<li>'
                    + '<a title="바로가기" href="'+ val.link +'">'
                    	+ '<img src="'+val.img_url+'" alt="'+ val.column_title +'">'
                    + '</a>'
                + '</li>';

                $list1.append(html);
            })
		}
			
		var data = res.outer;
		if (_.size(data) >= 1 && _.size(data) >= 1) {
            _.each(data,function (val) {

				var html ='<li>'
                    + '<a title="바로가기" href="'+ val.link +'">'
                    	+ '<img src="'+val.img_url+'" alt="'+ val.column_title +'">'
                    + '</a>'
                + '</li>';

                $list2.append(html);
			})
		}

		data = res.inner;
		if (_.size(data) >= 1 && _.size(data) >= 1) {
            _.each(data,function (val) {

				var html ='<li>'
                    + '<a title="바로가기" href="'+ val.link +'">'
                    	+ '<img src="'+val.img_url+'" alt="'+ val.column_title +'">'
                    + '</a>'
                + '</li>';

                $list3.append(html);
			})
		}
		
	});
	
});
</script>
	
<script>
$(document).ready(function() {

