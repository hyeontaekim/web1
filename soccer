<script>
$(document).ready(function() {
		
// 	지구촌 화제 등
	$.ajax({
		method: "GET",
//		url: "//ajax.news1.kr/www/?type=main_corners",
		url: "https://rest.news1.kr/w/ajax/extcontent?type=main_corners",
		dataType : "json",
		cache: false
	}).done(function(data) {
		var json = data,
			$list = $("#TopicSets");

		if (json.size >= 1) {
			for (var i in json.data) {
			// 최대 5개
				if (i >= 5)	break;
				
				var val = json.data[i],
					title_img = _image_root_ + "/aside/title_category" + val.image_code + ".png",
					listArr = [],
					top_str = "",
					li_str = "";
// console.log(val.title);
				if (val.size >= 1) {
					for (var j in val.data) {
						// 최대 4개
						if (j >= 4)	break;
                        if(typeof val.data[j] === 'function'){
                            return;
                        }
						var val2 = val.data[j],
							title = val2.title.decode().cut(48),
							content = val2.content.decode().cut(48),
							url = "/articles/?" + val2.article_id;
// console.log(val2);
						if (j == 0) {
							top_str = '<div class="thumb">'
									+ '<a href="' + url + '">'
									+ '<img style="width:100px;" onerror="javascript:this.src=\'' + _image_root_ + '/common/img_no_type2.gif\'" src="' + val2.img_url + '">'
									+ '</a>'
								+ '</div>'
								+ '<dl class="info">'
									+ '<dt><a href="' + url + '">' + title + '</a></dt>'
									+ '<dd><a href="' + url + '">' + content + '</a></dd>'
									+ '</dl>';
						} else {
							li_str += '<li><a href="' + url + '">' + title + '</a></li>';
							
						}
						
					}
				} // if (val.size >= 1)
				
