<script>
$(document).ready(function() {
		
// 	많이 본 기사
	$.ajax({
		method: "GET",
		url: "https://rest.news1.kr/w/ajax/extcontent?type=most_viewed_news",
		dataType : "json",
		cache: false
	}).success(function(data) {
		var json = data,
			$list = $('#MostHitNews'),
			$tab = $list.find(".tab_category"),
			$detail = $list.find(".rankNews");
		var _most_hits = [];
		
		if (json.size >= 1 && _.size(json.data) >= 1) {

			// 최대 15개
			var limit = 15;
			// 카테고리별
			for (var i in json.data) {
				if (i >= limit)	break;

				var val = json.data[i];
				if(typeof val == 'function'){
				    //ie에서 array function까지 루프탐
				   return
                }
				if (val.size <= 0 ||  _.size(val) <= 0)	continue;
				
				var addclass = (i == 0)? " on" : "",
					data_img = (val.data[0].img_url.isEmpty())? "off" : "on",
					cid = val.id;

                var el = '<li class="cat_' + cid + addclass + '" ><a data-id="' + cid + '" data-img="' + data_img + '" href="#"><span class="small">' + val.name + '</span></a></li>';
				if(cid == 144) {
                    var el = '<li class="cat_' + cid + addclass + '" ><a data-id="' + cid + '" data-img="' + data_img + '" href="#"><span>' + val.name + '</span></a></li>';
                }
				_most_hits[cid] = val;
				
				$tab.append(el);
				
				// 기사별
                _.each(val.data,function (val2,j) {
				// for(var j in val.data) {
					if ( cid == 'total' && j >= 10 )	{ return; } // 최대 9개
					if ( cid != 'total' && j >= 5 )	{ return; } // 최대 5개
					// var val2 = val.data[j];

// 					전체 카테고리일 경우
// 					if (val['id'] == "total") { //
						var num = parseInt(j) + 1,
							title = val2.title.decode(),
							url = "/articles/?" + val2.article_id,
							content = val2.content.decode().cut(80),
							$li;

						var add_style = (cid != 'total')? ' tab' + cid + ' none' : ' tab' + cid;
						var add_tag = '';
						if ( cid == 'total' ) {
							var curl = (val2.category_id == 145)? "/nstyle/" : "/categories/?" + val2.category_id;
							add_tag = '<a class="cat" href="' + curl + '">[' +val2.category_name + ']</a> ';
						}
// 						console.log(add_tag);
							
						if (num == "1" && cid != 'total' ) {
							if (val2.img_url.isEmpty()) {
								$li = $('<li class="no1' + add_style + '">'
										+ add_tag
										+ '<a href="' + url + '">'
											+ '<span class="newtitle ellipsis"></span>'
											+ '<em class="hidden">1</em>'
											+ '<div class="hitnews thumb" style="display:none; width:75px; height:48px;">'
												+ '<img style="overflow:hidden; height:48px; width:75px;" onerror="javascript:this.src=\'https://image.news1.kr/system/resource/news1_www/images/common/img_no_type2.gif\'" alt="" src="">'
											+ '</div>'
											+ '<span class="hitnews hitnews3"></span>'
										+ '</a>'
									+ '</li>');
								$li.find(" > a > .newtitle").text(title);
								$li.find(" > a > .hitnews3").text(content);
								
							} else {
								$li = $('<li class="no1' + add_style + '">'
											+ add_tag
											+ '<a href="' + url + '">'
												+ '<span class="newtitle ellipsis"></span>'
												+ '<em class="hidden">1</em>'
												+ '<div class="hitnews thumb" style="width:75px; height:48px;">'
													+ '<img style="overflow:hidden; height:48px; width:75px;" onerror="javascript:this.src=\'https://image.news1.kr/system/resource/news1_www/images/common/img_no_type2.gif\'" alt="" src="' + val2.img_url +'">'
												+ '</div>'
												+ '<span class="hitnews hitnews2"></span>'
											+ '</a>'
										+ '</li>');
								$li.find(" > a > .newtitle").text(title);
								$li.find(" > a > .hitnews2").text(content);
								
							}
								
						} else {
							$li = $('<li class="no' + num + add_style + ' ellipsis"><em class="hidden">' + num + '</em>'
								+ add_tag
								+ '<a href="' + url + '" ></a>'
								+ '</li>');
							$li.find(" > a").last().text(title);
						}
						$li.find("a").last().attr("href", url).attr("title", title);
						$detail.append($li);					
// 					} // 

				}) // for(var j in val[i])
						
			} // for (var i in json.data)

            $tab.append('<li><a href="#"><span></span></a></li>');

			_most_news_page = 0;
			$list.find(".arrow.prev").click(function(e) {
				e.preventDefault();
				if (_most_news_page <= 0) {
					var max =  parseInt(Math.min(limit - 5, json.size - 5) / 5); //  ( 0~2 )
					_most_news_page = max;
					$tab.animate({left: "-" + 265*(max) + "px"}, 'slow');
				} else {
					_most_news_page--;
					$tab.animate({left: "+=265px"}, 'slow');
				}
			});

			$list.find(".arrow.next").click(function(e) {
				e.preventDefault();
				var max =  parseInt(Math.min(limit - 5, json.size - 5) / 5); //  ( 0~2 )
				if (_most_news_page >= max) {
					_most_news_page = 0;
					$tab.animate({left: "0px"}, 'slow');
				} else {
					_most_news_page++;
					$tab.animate({left: "-=265px"}, 'slow');
				}
			});

			$tab.find("> li > a").click(function(e) {
				e.preventDefault();
				var $li = $(this).parent();
				$li.siblings().removeClass("on");
				$li.addClass("on");
				var cid = $(this).data("id"),
					hasImg = $(this).data("img");
// 				setMostHitTab(cid, hasImg);
				setMostHitTab_New(cid);
			});

		} // json.size >= 1
    }).error(function (request) { // .success()
        $list = $('#MostHitNews');
		$list.hide();
	});

}); // $(document).ready


function setMostHitTab_New(id) {
    if(typeof id == 'undefined' || id == null) {
        return ;
    }
	$list = $("#MostHitNews .rankNews");
	$list.find('li').addClass('none');
	$list.find('.tab'+id).removeClass('none');

	$list = $("#MostHitNews .tabDetail");
	if (id == 'total') {
		$list.addClass('tabd_total');
	} else {
		$list.removeClass('tabd_total');
		
	}
}


function setMostHitTab(id, hasImg) {
	var data = _most_hits[id],
		$list = $("#MostHitNews .rankNews");
// console.log(data);
// 	for (var i in data.data) {
	    _.each(data.data,function (val,i) {

		// var val = data.data[i],
        var num = parseInt(i) + 1,
			$li = $list.find(".no" + num),
			title = val['title'].decode(),
			url = "/articles/?" + val['article_id'];

		if (i == 0) {
			var $content = $li.find(" .hitnews").last(),
				content = val['content'].decode().cut(80);
			if (hasImg == "on") {
				$content.addClass("hitnews2").removeClass("hitnews3");
				$li.find(".thumb > img").attr("src", val['img_url']);
				$li.find(".thumb").show("fast");
			} else {
				$content.addClass("hitnews3").removeClass("hitnews2");
				$li.find(".thumb").hide();
			}
			$li.find(" > a > .newtitle").text(title);
			$content.text(content);
		} else {
			$li.find("> a").text(title);
		}
		$li.find("a").attr("href", url).attr("title", title);

	})
	
}
