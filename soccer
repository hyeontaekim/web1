            top: 0;
            right: 0;
            left: 0;
            bottom: 0;
            opacity: 0;
        z-index: 11}
    </style>
    <script>
        $(document).ready(function () {
            fetchLatestData()
            // $(".modal_layer").on('click',function () {
            //     $('.box_list_news').hide();
            //     $(".modal_layer").hide();
            //     $('.wrap_list_realtime .open_list .box_list_news').css('opacity',0)
            // })
        });
        function fetchLatestData() {
            var now = new Date();
            var month  = now.getMonth()+1;
            var date = now.getFullYear()+"-"+(month<10?"0"+month:month)+"-"+now.getDate()
            var searchParams = {page: 1, pg_per_cnt: 20}
            $.get("https://rest.news1.kr/archive/list", searchParams, function (data) {
                var totalCount = data['total'],
                    lastPage = data['last_page'],
                    currentPage = data['current_page'],
                    json = data['data'];
                json = json.splice(0,10)
                if (json.length > 0) {
                    $(".news_list_realtime").slick('removeSlide', null, null, true);
                    for (var i = 0; i < json.length; i++) {
                        var item = json[i];
                        $(".news_list_realtime").slick('slickAdd', '<a href="/articles/?' + item.id +'" class="slider"><span class="txt">' + item.title + '</span>'+'<span class="time">'+item.pubdate1+'</span></a>');
                    }
                    // renderSection("div.news_list_realtime", "#sliderListTemplate", {items: json});
                    $("div.box_list_news").empty();
                    renderSection("div.box_list_news", "#sliderListTemplate", {items: json});

                    // $(".wrap_list_realtime").show();
                } else {
                    // $(".wrap_list_realtime").hide();
                    ga('newver_news1.send', 'exception', {'type':'empty','page':location.href,'method':'fetchLatestData','data':data});
                }
                setTimeout(fetchLatestData, 1*60 * 1000)
            }).fail(function(data) {
                ga('newver_news1.send', 'exception', {'type':'fail','page':location.href,'method':'fetchLatestData','data':data});

            })
        }

    </script>
</div>
