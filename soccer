                            <script type="text/javascript">
                                $(document).ready(function() {
                                    // 검색박스
                                    $("#DisplaySearchLabel").click(function(e) {
                                        $("#SearchOptionBox").toggleClass("on off");
                                        e.preventDefault();
                                        e.stopPropagation();
                                    });

                                    $(document).click(function(){
                                        $("#SearchOptionBox").removeClass("on").addClass("off");
                                    });

                                    $("#SoTypeAll, #SoTypeWriter, #SoTypeTitleContent, #SoTypeTitle, #SoTypeContent").click(function(e) {
                                        var txt = $(this).data("txt");
                                        $("#DisplaySearchLabel").text(txt);
                                        $("#SearchOptionBox").toggleClass("on off");
                                    });
                                });
                            </script>

